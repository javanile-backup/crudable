{
    "name": "javanile/crubable",
    "description": "Ready to use CRUD Server to store data into MySQL",
    "version": "0.0.2",
    "license": "MIT",
    "type": "application",
    "authors": [
        {
            "name": "Francesco Bianco",
            "email": "bianco@javanile.org"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/javanile/crudable"
        }
    ],
    "autoload": {
        "psr-4": {
            "Javanile\\Crudable\\": "src/Crudable"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javanile\\Crudable\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^5.6.0 || >=7.0 <7.2",
        "slim/slim": "4.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "javanile/producer": "^0.0.18",
        "codeclimate/php-test-reporter": "dev-master"
    }
}